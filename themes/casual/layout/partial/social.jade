.social-links
  ul
    each value, key in theme.social
      - var re = /^(http|https):\/\/*/gi;
      - var tar = re.test(value) ? "_blank" : "_self"
      li
        a(
          href=url_for(value)
          target=tar
          title=__(key)
          class='link-#{key}'
        ).social-link
          i(class='fa fa-#{key}')
